pkgname="linux-planet-geminipda"
pkgver=3.18
pkgrel=0
pkgdesc="Gemini PDA kernel blob"
arch="aarch64"
_carch="arm64"
_flavor="planet-geminipda"
url="https://kernel.org"
license="GPL2"
options="!strip !check !tracedeps"
# Source
source="zImage"

package() {
	# kernel.release
	install -d "$pkgdir/usr/share/kernel/$_flavor"
	echo $pkgver > "$pkgdir/usr/share/kernel/$_flavor/kernel.release"

	_target="$pkgdir/boot/vmlinuz-$_flavor"
	install -Dm644 "$srcdir/zImage" "$_target"

	# cd "$pkgdir"
    # rpmextract.sh "$srcdir/$_modules_rpm"
}

